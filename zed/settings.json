{
    "features": {
        "edit_prediction_provider": "none"
    },
    "collaboration_panel": {
        "dock": "right"
    },
    "outline_panel": {
        "dock": "right"
    },
    "git_panel": {
        "dock": "right"
    },
    "project_panel": {
        "dock": "right"
    },
    "ssh_connections": [],
    "diagnostics": {
        "button": true,
        "include_warnings": true,
        "inline": {
            "enabled": true,
            "update_debounce_ms": 150,
            "padding": 4,
            "min_column": 0
        }
    },
    "lsp": {
        "rust-analyzer": {
            "binary": {
                "ignore_system_version": false
            },
            "enable_lsp_tasks": true,
            "initialization_options": {
                "checkOnSave": true
            }
        }
    },
    "tab_size": 4,
    "autosave": "on_focus_change",
    "agent": {
        "always_allow_tool_actions": true,
        "default_profile": "write",
        "inline_assistant_model": null,
        "enabled": true
    },
    "format_on_save": "on",
    "file_types": {
        "C": ["c", "h"]
    },
    "languages": {
        "TypeScript": {
            "language_servers": ["eslint"]
        },
        "C": {
            "tab_size": 4,
            "formatter": {
                "external": {
                    "command": "clang-format",
                    "arguments": [
                        "--style=file",
                        "--assume-filename={buffer_path}"
                    ]
                }
            },
            "format_on_save": "on"
        },
        "PHP": {
            "format_on_save": "off"
        }
    },
    "vim_mode": true,
    "ui_font_size": 16,
    "buffer_font_size": 14,
    "theme": {
        "mode": "system",
        "light": "One Light",
        "dark": "One Dark"
    }
}
